<Window x:Class="DynamicDataExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:model="clr-namespace:DynamicDataExample.Model"
        xmlns:local="clr-namespace:DynamicDataExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <model:MainViewModel />
    </Window.DataContext>
    <DockPanel LastChildFill="True">
        <ListBox ItemsSource="{Binding ListItems}" DisplayMemberPath="Content" />
    </DockPanel>
</Window>
